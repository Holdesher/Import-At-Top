{
	"name": "import-at-top",
	"publisher": "kah3vich",
	"displayName": "Import At Top",
	"description": "...",
	"repository": "https://github.com/kah3vich/Import-At-Top",
	"version": "0.0.15",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:import-at-top"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "Import At Top",
			"properties": {
				"import-at-top.config": {
					"type": "array",
					"default": []
				}
			}
		},
		"commands": [
			{
				"command": "import-at-top",
				"title": "Import At Top"
			}
		]
	},
	"scripts": {
		"build": "vsce package",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
