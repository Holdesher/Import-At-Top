{
	"name": "import-at-top",
	"publisher": "kah3vich",
	"displayName": "Import At Top",
	"description": "...",
	"repository": {
		"type": "git",
		"url": "https://github.com/kah3vich/Import-At-Top"
	},
	"version": "0.0.21",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"preview": false,
	"galleryBanner": {
		"color": "#ffffff",
		"theme": "light"
	},
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "Import At Top",
			"properties": {
				"import-at-top.config": {
					"type": "array",
					"description": ".",
					"default": [
						{
							"trigger": "useState",
							"packet": "react",
							"defaultType": false
						}
					]
				}
			}
		},
		"commands": [
			{
				"command": "import-at-top",
				"title": "Import At Top"
			}
		],
		"keybindings": [
			{
				"command": "import-at-top",
				"key": "shift+i",
				"when": "editorTextFocus",
				"mac": "shift+i"
			}
		]
	},
	"activationEvents": [
		"onCommand:import-at-top"
	],
	"icon": "assets/logo.png",
	"license": "GPL-3.0-only",
	"author": {
		"email": "test@gmail.com",
		"name": "Nik Bild",
		"url": "https://github.com/kah3vich"
	},
	"bugs": {
		"url": "https://github.com/kah3vich/Import-At-Top/issues",
		"email": "test@gmail.com"
	},
	"keywords": [
		"import",
		"import top",
		"import at top",
		"import-top",
		"import-at-top",
		"auto-import",
		"import-auto",
		"import auto",
		"auto import"
	],
	"homepage": "",
	"scripts": {
		"build": "vsce package",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.60.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"@types/eslint": "^8.21.0",
		"@types/prettier": "^2.7.2",
		"eslint": "^7.32.0",
		"prettier": "^2.8.4"
	}
}
